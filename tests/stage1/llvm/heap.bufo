import "prelude.bufo";
import "libc.bufo";

struct Foo {
    f1: usize;
    f2: usize;
    f3: usize;
    f4: usize;
    f5: usize;
}

func newFoo() -> Foo {
    return Foo {
        f1: 0,
        f2: 0,
        f3: 0,
        f4: 0,
        f5: 0,
    };
}

func main(argc: i32, argv: &&char) -> i32 {
    if (sizeof Foo != 40) return 1;

    let f: Foo = newFoo();
    let _idk: Any = calloc(2, sizeof Foo);
    assert(_idk != null);
    let _b: &Foo = _idk;
    *(_b + sizeof Foo) = f;
    free(_idk);
    return 0;
}
