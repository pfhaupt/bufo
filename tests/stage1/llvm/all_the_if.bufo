func foo() -> u32 {
    let a: u32 = 1;
    if (a < 0) {
        return 0;
    } else return 1;
}

func bar(a: u32) -> u32 {
    if (a == 420) if (5 == 5) return 1; else {
        return 2;
    }
    if (a == 69) {
        return 5;
    } else return 1;
}

func main(argc: i32, argv: &&char) -> i32 {
    let a: u32 = 10;
    let b: u32 = 10;
    let c: u32 = 5;
    if (a == b) c = foo(); // sets c to 1
    else {}
    if (c == 1)
        if (b == 10)
            if (a == 9)
                return 1;
            else {
                a = bar(69); // sets a to 5
            }
        else return 1;
    else {
        if (a == 5) {
            return 5;
        }
        return 1;
    }
    return 0;
}
