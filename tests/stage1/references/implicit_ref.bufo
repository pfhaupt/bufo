import "prelude.bufo";

struct Foo {
    b: Bar;
}

struct Bar {
    f: i32;
    func setF(&mut this, f: i32) {
        this.f = f;
    }
}

func main(argc: i32, argv: &&char) -> i32 {
    mut f: Foo = Foo {
        b: Bar {
            f: 10
        }
    };
    f.b.setF(5);
    assert(f.b.f == 5);
    return 0;
}
