import "prelude.bufo";

func main(argc: i32, argv: &&char) -> i32 {
    let counter: i32 = 0;
    while (counter < 100) {
        if (counter == 10) break;
        counter = counter + 1;
    }
    assert(counter != 100);
    assert(counter == 10);
    return 0;
}
