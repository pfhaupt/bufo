import "prelude.bufo";
import "libc.bufo";

struct Foo {
    b1: bool;
    b2: bool;
}

func main(argc: i32, argv: &&char) -> i32 {
    let b = true;
    match (b) {
        true => { printf("b yep\n"); }
        false => { return 1; }
        _ => { return 2; }
    }
    let foo = Foo { b1: true, b2: false };
    match (foo) {
        Foo { false, false } => { return 1; }
        Foo { true , false } => { printf("foo yep\n"); }
        Foo { false, true  } => { return 1; }
        Foo { true , true  } => { return 1; }
        _ => { return 2; }
    }
    assert(foo.b1);
    assert(!foo.b2);
    return 0;
}
