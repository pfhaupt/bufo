struct Foo {
    ptr: &i32;

    func getValue(&this) -> i32 {
        unsafe {
            if (this.ptr != null) {
                return *(this.ptr);
            } else {
                return -1;
            }
        }
    }
}

unsafe func main() {
    mut f: Foo = Foo {
        ptr: null
    };
    assert(f.getValue() == -1);
    f.ptr = &5;
    assert(f.getValue() == 5);
    f.ptr = null;
    assert(f.getValue() == -1);
}
